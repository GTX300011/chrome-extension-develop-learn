{
    "manifest_version" : 3,
    "name" : "Read Times",
    "version" : "1.0.0",

    "description" : "statistics web read times",
    "icons" : {
        "16" : "icons/16.png",
        "32" : "icons/32.png",
        "48" : "icons/48.png",
        "128" : "icons/128.png"
    },

    "content_scripts" : [
        {
            "js" : ["scripts/content.js"],
            "matches" : [
                "https://developer.chrome.com/docs/extensions/*",
                "https://developer.chrome.com/docs/webstore/*"
            ]
        }
    ]

}
